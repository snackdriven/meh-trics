{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "env": {
    "es2021": true,
    "node": true
  },
  "ignorePatterns": [
    "node_modules",
    "dist"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "error"
  }
}
