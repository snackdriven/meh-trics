{
  "name": "meh-trics-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "analyze": "npx vite-bundle-analyzer dist/assets/*.js",
    "lint": "bunx @biomejs/biome lint .",
    "lint:fix": "bunx @biomejs/biome lint --apply .",
    "format": "bunx @biomejs/biome format --write .",
    "preview": "vite preview",
    "test": "vitest",
    "perf:measure": "npm run build && npm run preview --host & sleep 2 && npx lighthouse http://localhost:4173 --output=json --output-path=./performance-report.json && pkill -f vite"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tanstack/react-query": "^4.39.2",
    "ai": "^4.3.16",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "idb": "^8.0.3",
    "lucide-react": "^0.294.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@alloc/quick-lru": "^5.2.0",
    "@esbuild/linux-x64": "^0.21.5",
    "@rollup/rollup-linux-x64-gnu": "^4.43.0",
    "@types/canvas-confetti": "^1.6.4",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vite-bundle-analyzer": "^0.18.0",
    "vitest": "^1.6.1"
  }
}
